<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

  <servlet>
    <servlet-name>parsing</servlet-name>
    <servlet-class>com.alex.webParsing.ParsingText</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>parsing</servlet-name>
    <url-pattern>/parsing</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>returnToForm</servlet-name>
    <servlet-class>com.alex.webParsing.ReturnToForm</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>returnToForm</servlet-name>
    <url-pattern>/returnToForm</url-pattern>
  </servlet-mapping>

</web-app>
